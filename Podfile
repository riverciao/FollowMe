  pod 'Firebase/Storage'
  pod 'Firebase/Database'
  pod 'YXWaveView'
  pod 'NVActivityIndicatorView'

  # Pods for PodTest
  pod 'Fabric', '~> 1.7.2'
  pod 'Crashlytics', '~> 3.9.3'

end
